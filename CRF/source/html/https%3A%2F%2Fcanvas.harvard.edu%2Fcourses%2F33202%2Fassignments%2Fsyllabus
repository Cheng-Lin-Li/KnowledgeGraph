<div role="complementary" aria-label="Assignment Weights">
  <h2>Assignments are weighted by group:</h2>

  <table class="summary">
    <thead>
    <tr>
      <th scope='col'>Group</th>
      <th scope='col'>Weight</th>
    </tr>
    </thead>
    <tbody>
        <tr>
          <th scope='row'>Assignments</th>
          <td>0%</td>
        </tr>
        <tr>
          <th scope='row'>Other</th>
          <td>0%</td>
        </tr>
        <tr>
          <th scope='row'>Missions</th>
          <td>20%</td>
        </tr>
        <tr>
          <th scope='row'>Midterm Exam</th>
          <td>20%</td>
        </tr>
        <tr>
          <th scope='row'>Final Project</th>
          <td>30%</td>
        </tr>
        <tr>
          <th scope='row'>Final Exam</th>
          <td>20%</td>
        </tr>
        <tr>
          <th scope='row'>Participation (Weekly Quizzes)</th>
          <td>10%</td>
        </tr>
      <tr style="font-weight: bold;">
        <th scope='row'>Total</th>
        <td>100%</td>
      </tr>
    </tbody>
  </table>
