<div role="complementary" aria-label="Assignment Weights">
  <h2>Assignments are weighted by group:</h2>

  <table class="summary">
    <thead>
    <tr>
      <th scope='col'>Group</th>
      <th scope='col'>Weight</th>
    </tr>
    </thead>
    <tbody>
        <tr>
          <th scope='row'>Class Information/ Instruction</th>
          <td>0%</td>
        </tr>
        <tr>
          <th scope='row'>Informative Speech Presentation</th>
          <td>15%</td>
        </tr>
        <tr>
          <th scope='row'>Informative Speech Outline</th>
          <td>5%</td>
        </tr>
        <tr>
          <th scope='row'>Persuasive Speech Presentation </th>
          <td>20%</td>
        </tr>
        <tr>
          <th scope='row'>Persuasive Speech Outline</th>
          <td>5%</td>
        </tr>
        <tr>
          <th scope='row'>Final Speech Presentation</th>
          <td>25%</td>
        </tr>
        <tr>
          <th scope='row'>Final Speech Outline</th>
          <td>5%</td>
        </tr>
        <tr>
          <th scope='row'>Examination</th>
          <td>15%</td>
        </tr>
        <tr>
          <th scope='row'>Class Attendance and Particpation</th>
          <td>10%</td>
        </tr>
      <tr style="font-weight: bold;">
        <th scope='row'>Total</th>
        <td>100%</td>
      </tr>
    </tbody>
  </table>
</div>

